{
  "branches": [
    "*"
  ],
  "dryRun": true,
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "conventionalcommits"
      }
    ],
    [
      "@semantic-release/release-notes-generator",
      {
        "preset": "conventionalcommits"
      }
    ],
    [
      "@semantic-release/github",
      {
        "successComment": "ðŸš€ This is a dry run! The next release version will be **${nextRelease.version}**.",
        "releasedLabels": [
          "released"
        ]
      }
    ]
  ]
};